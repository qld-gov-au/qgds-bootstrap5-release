
    <a class="qld-header-brand" href="{{url}}">
        <div class="qld-header-brand-image {{#ifAny isSubBrand isCoBrand isEndorsedBrand}}is-subtype{{/ifAny}}">
            {{#if logo}}
            <img src="{{logo.src}}" width="330" height="56" alt="{{#if logo.altText}}{{logo.altText}}{{else}}Queensland government{{/if}}" />
            {{else}}
            {{#if siteTitle}}
            {{>logo logo="coa-landscape-2lines" height="56" fill="currentColor"}}
            {{else}}
             {{>logo height="56" fill="currentColor"}}
            {{/if}}
            {{/if}}
        </div>
        {{#if secondaryLogo}}
            <div class="qld-header-site-name">
                <img class="qld-header-secondary-image" alt="{{#if secondaryLogo.altText}}{{secondaryLogo.altText}}{{else}}{{siteTitle}}{{#if subline}}{{subline}}{{/if}}{{/if}}"
                    src="{{secondaryLogo.src}}" />
            </div>
        {{else}}
        {{#if siteTitle}}
        <div class="qld-header-site-name">
            <span class="qld-header-heading">
                {{siteTitle}}
            </span>
            {{#if subline}}
            <span class="qld-header-subline">
                {{subline}}
            </span>
            {{/if}}
        </div>
        {{/if}}
        {{/if}}
    </a>
