<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>{{title}}</title>
  {{> head }}
</head>
<body>
    {{> globalAlert globalAlert}}
    {{> header header}}
    {{> navbar navbar}}    
    <main>
        <div class="container-fluid">
            <div class="container qld-content">
                <div class="row">
                    <!-- Side Nav -->
                    {{#> sidenavWrapper}}
                    {{> sidenav sidenav}}
                    {{/sidenavWrapper}}
                    {{#> contentWrapper}}
                        <p>content goes here dd</p>
                        {{> table table}}                        
                    {{/contentWrapper}}
                </div>                    
            </div>
            <div class="container">
                {{> contentFooter contentFooter }}
            </div>
        </div>
    </main>
    {{> footer footer}}
</body>
</html>