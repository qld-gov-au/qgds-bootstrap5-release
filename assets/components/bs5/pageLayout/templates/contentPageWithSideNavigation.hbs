<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>{{title}}</title>
  {{> head }}
</head>
<body>
    {{> header header}}
    {{> navbar navbar}}
    {{> globalAlert globalAlert}}
    
    {{#if banner}}
    {{> banner banner}}
    {{/if}}
    
    <main>
        <div class="container-fluid">
            <div class="container qld-content">
                {{#if breadcrumbs}}
                <div class="row">
                    <div class="col-12">
                        {{> breadcrumbs breadcrumbs}}
                    </div>
                </div>
                {{/if}}
                
                <div class="row">
                    {{#if sidenav}}
                    <div class="col-lg-3 col-md-4">
                        {{> sidenav sidenav}}
                    </div>
                    <div class="col-lg-9 col-md-8">
                    {{else}}
                    <div class="col-12">
                    {{/if}}
                        {{#if inpagenav}}
                        {{> inpagenav inpagenav}}
                        {{/if}}
                        
                        <div class="content-section">
                            {{{content}}}
                        </div>
                        
                        {{#if image}}
                        <div class="my-4">
                            {{> image image}}
                        </div>
                        {{/if}}
                        
                        {{#if table}}
                        <div class="my-4">
                            {{> table table}}
                        </div>
                        {{/if}}
                        
                        {{#if video}}
                        <div class="my-4">
                            {{> video video}}
                        </div>
                        {{/if}}
                        
                        {{> contentFooter contentFooter}}
                    </div>
                </div>
                
                <div class="row">
                    <div class="col-12">                        
                        {{> backToTop backToTop}}
                    </div>
                </div>                                        
            </div>
        </div>
    </main>
    {{> footer footer}}
</body>
</html>