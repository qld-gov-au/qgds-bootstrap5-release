<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>{{title}}</title>
  {{> head }}
</head>
<body>
    {{> header header}}
    {{> navbar navbar}}
    {{> globalAlert globalAlert}}
    <main>
        <div class="container-fluid">
            <div class="container qld-content">
                <div class="row">
                    {{#> contentWrapper}}
                        <p>content goes here dd</p>
                        {{> table table}}
                    {{/contentWrapper}}

                    {{#>contentFooterWrapper}}
                        {{> backToTop backToTop}}
                    {{/contentFooterWrapper}}
                </div> <!-- end row -->
            </div>
        </div>
    </main>
    {{> footer footer}}
</body>
</html>