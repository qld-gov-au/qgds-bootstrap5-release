<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>{{title}}</title>
  {{> head }}
</head>

<body>

  <div class="qld-flex-grid">

    {{!-- {{> globalAlert globalAlert}} --}}
    {{> header header}}
    {{> navbar navbar}}
    
    <main id="content">

        <!-- breadcrumbs-->
        {{#if breadcrumbsData }}
        <div class="container-fluid alt px-0">
            <div class="container-full">
              {{> breadcrumbs breadcrumbsData }}        
            </div>
        </div>
        {{/if}}

        <!-- Content Wrapper -->
        <div class="container-full mt-40 mt-lg-64">
          <div class="row">
          
          <!-- Side Navigation -->
          {{#if sidenavData }}
          <div class="col-12 col-lg-3 pe-lg-0 order-last order-lg-first mt-5 mt-lg-0">
          {{> sidenav sidenavData }}
          </div>
          {{/if}}


          <!-- Content body -->
          <div class="col-12 col-md-8 px-lg-32 qld-content-body">

            {{!-- Main content area --}}
            <h1>{{title}}</h1>

            {{#if abstract}}
            <div class="qld-abstract">
              {{{ abstract }}}
            </div>
            {{/if}}

            {{> inpagenav inpageNavData }}

            <h2 id="section-one">Section heading</h2>
